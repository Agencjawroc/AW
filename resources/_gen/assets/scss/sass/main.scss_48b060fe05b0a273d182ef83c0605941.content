e}.txt-underline>*:not(.txt-underline){text-decoration:none}.sc-two-col-img-txt .two-col-row{display:flex;flex-wrap:wrap;flex-direction:row}.sc-two-col-img-txt .two-col-row .second-col iframe{width:100%}.sc-two-col-img-txt .two-col-row .second-col img{width:100%}.sc-two-col-img-txt .two-col-row .second-col svg{width:100%}@media(min-width:1200px){.sc-two-col-img-txt .two-col-row>div:first-child{width:45%}.sc-two-col-img-txt .two-col-row>div:last-child{width:55%}.sc-two-col-img-txt .two-col-row:nth-child(even){flex-direction:row-reverse}}.txt-red{color:var(--red);transition:color .3s ease}.txt-black{color:var(--main-dark-color);transition:color .3s ease}body{color:var(--main-dark-color);background-color:var(--white);transition:background-color .3s ease,color .3s ease}body.dark-m{background-color:#000;color:var(--white)}body.dark-m .txt-red{color:var(--red)}body.dark-m .txt-black{color:var(--white)}